<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /visa_f/
    
    # Redirect all requests to index.php
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php [QSA,L]
</IfModule>

# Security: Prevent directory listing

Options -Indexes
